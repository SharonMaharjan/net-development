@model MvcMoviePractice.Models.Movie
@{
    ViewData["Title"] = "Create";
    var ratings = (IEnumerable<SelectListItem>)ViewData["Ratings"]!;
}
<form asp-action="Create">
    <label asp-for="Title">@Html.DisplayNameFor(model=>model.Title)</label>
    <input asp-for="Title"/><br/>
    <label asp-for="Rating">@Html.DisplayNameFor(model=>model.Rating)</label>
    <select asp-for="RatingID" asp-items="ratings"><</select>
    <br/>
    <input type="submit" value="Create"/>
</form>